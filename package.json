{
  "name": "tool-bar",
  "version": "1.3.0",
  "description": "Package providing customisable tool bar",
  "keywords": [
    "toolbar",
    "tool-bar",
    "service"
  ],
  "license": "MIT",
  "main": "./dist/tool-bar",
  "repository": {
    "type": "git",
    "url": "https://github.com/suda/toolbar.git"
  },
  "scripts": {
    "lint": "eslint . --fix --ext js,json && remark *.md",
    "test": "atom --test spec",
    "clean": "shx rm -rf dist",
    "debug": "npm run clean && shx cp -r lib dist && NODE_ENV=development BABEL_ENV=development babel dist --out-dir dist",
    "dev": "npm run clean && NODE_ENV=development BABEL_ENV=production rollup -c -w",
    "build": "npm run clean && NODE_ENV=production BABEL_ENV=production rollup -c",
    "ppublish": "bash ./ppublish.sh",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/atom": "latest",
    "eslint": "^6.8.0",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-json": "^2.1.1",
    "remark-cli": "^8.0.0",
    "remark-preset-lint-recommended": "^4.0.0",
    "rollup": "^2.11.2",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "shx": "^0.3.2",
    "@babel/core": "^7.10.1",
    "@babel/cli": "^7.10.1",
    "@babel/preset-env": "^7.10.1"
  },
  "engines": {
    "atom": ">=1.20.0"
  },
  "providedServices": {
    "tool-bar": {
      "description": "A container for buttons at workspace edge",
      "versions": {
        "1.0.0": "provideToolBar"
      }
    }
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ]
}
